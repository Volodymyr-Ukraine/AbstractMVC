# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'LibMVC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LibMVC
pod 'Alamofire', '~> 5.0.0-beta.5'
pod 'Nimble', '~> 6.0.0'
pod 'Quick'
pod 'SnapKit', '~> 4.0.0'


  target 'LibMVCTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Alamofire', '~> 5.0.0-beta.5'
pod 'Nimble', '~> 6.0.0'
pod 'Quick'
pod 'SnapKit', '~> 4.0.0'

  end

end

target 'mvc' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for mvc
pod 'Alamofire', '~> 5.0.0-beta.5'
pod 'Nimble', '~> 6.0.0'
pod 'Quick'
pod 'SnapKit', '~> 4.0.0'

  target 'mvcTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
